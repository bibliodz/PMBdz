<manifest>
	<capacities>
		<nostates>
			<state name="STOPPED"/>
			<state name="SUSPENDED"/>
		</nostates>
		<states>
			<state name="RUNNING">
				<nocommand commands="SUSPEND"/>
				<nocommand commands="ABORT"/>
				<nocommand commands="STOP"/>
			</state>
		</states>
		<commands>
			<command />
		</commands>
	</capacities>
	<directory_upload>0</directory_upload>
</manifest>
