<manifest>
	<name>pmbesDatabase</name>
	<description>msg:pmbesDatabase_description</description>
	<requirements/>
	<methods>
		<method name="get_version_informations" rights="" version="1.0" language_independant="false" comment="msg:get_version_informations_description">
			<inputs />
			<outputs>
				<description>msg:get_version_informations_output_description</description>
				<result name="currentVersion" type="scalar" dataType="string" description="msg:get_version_informations_output_current_version"/>
				<result name="shouldbeVersion" type="scalar" dataType="string" description="msg:get_version_informations_output_shouldbe_version"/>
			</outputs>
		</method>		
		<method name="need_update" rights="" version="1.0" language_independant="false" comment="msg:need_update_description">
			<inputs />
			<outputs>
				<result name="need" type="scalar" dataType="boolean" description="msg:need_update_output_need"/>
			</outputs>
		</method>
		<method name="update" rights="" version="1.0" language_independant="false" comment="msg:update_description">
			<inputs />
			<outputs>
				<result name="result" type="scalar" dataType="boolean" description="msg:update_output_result"/>
				<result name="informations" type="scalar" dataType="string" description="msg:update_output_informations"/>
			</outputs>
		</method>
	</methods>
</manifest>
