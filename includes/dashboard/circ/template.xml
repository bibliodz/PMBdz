<template default_lang='fr_FR'>
	<content lang='fr_FR'><![CDATA[
		<h2>Emprunteurs</h2>
		<div class='row'>
			<div class='title'>Total : </div>
			<div class='value'>{{emprs.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>Abonnements expirés : </div>
			<div class='value'>{{emprs.0.expired}}</div>
		</div>
		<div class='row'>&nbsp;</div>
		<h2>Prets</h2>
		<div class='row'>
			<div class='title'>En cours : </div>
			<div class='value'>{{prets.0.total}}</div>
		</div>
		<div class='row'>
			<div class='title'>En retard : </div>
			<div class='value'>{{prets.0.late}}</div>
		</div>
		<div class='row'>&nbsp;</div>
		<h2>Réservation</h2>
		<div class='row'>
			<div class='title'>A valider : </div>
			<div class='value'>{{resas.0.a_valider}}</div>
		</div>
		<div class='row'>
			<div class='title'>Confirmées : </div>
			<div class='value'>{{resas.0.confirmee}}</div>
		</div>
		]]>
	</content>
</template>