	<matches>
		<match type="simple">
			<criteria name="Author" unimarcField="7XX"/>
			<criteria name='Publisher' unimarcField='210$c'/>
			<criteria name='Title' unimarcField='200$a,461$t'/>
			<criteria name='Artist' unimarcField='7XX'/>
			<criteria name='Composer' unimarcField='7XX'/>
			<criteria name='Conductor' unimarcField='7XX'/>
			<criteria name='MusicLabel' unimarcField='210$c'/>
			<criteria name='Orchestra' unimarcField='7XX'/>
			<criteria name='Actor' unimarcField='7XX'/>
			<criteria name='Director' unimarcField='7XX'/>
			<criteria name='Keywords' unimarcField='ALLOTHERS'/>
			<criteria name='Code' unimarcField='010$a'/>
		</match>
		<match type="power">
			<criteria name='author' unimarcField='7XX' excludeOperators="EXACT,STARTWITH"/>
			<criteria name='author-exact' unimarcField='7XX' operators="EXACT"/>
			<criteria name='author-begins' unimarcField='7XX' operators="STARTWITH"/>
			<criteria name='language' unimarcField='101$a,101$c'/>
			<criteria name='publisher' unimarcField='210$c'/>
			<criteria name='subject' unimarcField='60X,610' excludeOperators="CONTAINS_AT_LEAST,STARTWITH"/>
			<criteria name='subject-words-begin' unimarcField='60X,610' operators="CONTAINS_AT_LEAST"/>
			<criteria name='subject-begins' unimarcField='60X,610' operators="STARTWITH"/>
			<criteria name='title' unimarcField='200$a' excludeOperators="CONTAINS_AT_LEAST,STARTWITH"/>
			<criteria name='title-words-begin' unimarcField='200$a' operators="CONTAINS_AT_LEAST"/>
			<criteria name='title-begins' unimarcField='200$a' operators="STARTWITH"/>
			<!--<criteria name='keywords' unimarcField='ALLOTHERS' excludeOperators="STARTWITH"/>
			<criteria name='keywords-begin' unimarcField='ALLOTHERS' operators="STARTWITH"/>-->
			<criteria name='keywords' unimarcField='ALLOTHERS'/>
		</match>
	</matches>
